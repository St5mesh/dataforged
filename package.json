{
  "name": "dataforged",
  "version": "0.4.0",
  "description": "Official Ironsworn: Starforged rules data in JSON, for use in community tools.",
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "files": [
    "dist",
    "docs",
    "img",
    "markdown"
  ],
  "type": "module",
  "types": "./dist/dataforged.d.ts",
  "scripts": {
    "flush-dist": "rm -R ./dist && mkdir ./dist",
    "flush-image": "rm -R ./img && mkdir ./img",
    "build": "yarn flush-dist && npx ttsc && api-extractor run",
    "build-json": "node ./dist/start.js --enable-source-maps",
    "build-json-ironsworn": "node ./dist/start-ironsworn.js --enable-source-maps",
    "copy-json": "find ./dist -name \\*.json -exec cp {} . \\; && find ./dist -name \\*.json -exec cp {} ./src \\;",
    "start": "yarn json && yarn copy-json",
    "docs": "rm -R ./docs && mkdir ./docs && npx typedoc"
  },
  "directories": {
    "doc": "./docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rsek/dataforged.git"
  },
  "keywords": [
    "ironsworn",
    "starforged",
    "TTRPG"
  ],
  "author": "rsek",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/rsek/dataforged/issues"
  },
  "homepage": "https://github.com/rsek/dataforged#readme",
  "devDependencies": {
    "@types/deep-freeze-strict": "^1.1.0",
    "@types/fs-extra": "^9.0.13",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonpath": "^0.2.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.23",
    "@types/prettier": "^2.4.3",
    "@types/sharp": "^0.29.5",
    "@types/source-map-support": "^0.5.4",
    "@types/svgo": "^2.6.1",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "deep-freeze-strict": "^1.1.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.8.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-lodash": "^7.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-sort-class-members": "^1.14.1",
    "eslint-plugin-tsdoc": "^0.2.14",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.0.1",
    "lodash-es": "^4.17.21",
    "prettier": "^2.3.0",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.30.2",
    "source-map-support": "^0.5.21",
    "svgo": "^2.5.0",
    "ts-node": "^10.5.0",
    "tslog": "^3.3.3",
    "ttypescript": "^1.5.13",
    "typed-regex": "^0.0.8",
    "typedoc": "^0.22.13",
    "typescript": "^4.7.0-dev.20220331",
    "typescript-is": "^0.19.0",
    "typescript-transform-paths": "^3.3.1"
  },
  "dependencies": {}
}
